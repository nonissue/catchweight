{
  "name": "with-typescript",
  "version": "0.1.1",
  "scripts": {
    "dev": "next",
    "build": "run-s \"build:**\"",
    "build:scripts": "node scripts/build.js",
    "build:web": "next build",
    "build:list": "ls -la public/",
    "clean": "run-s \"clean:assets\"",
    "clean:assets": "rimraf \"public/!(.gitkeep)\"",
    "clean:next": "rimraf \".next\"",
    "start": "next start",
    "type-check": "tsc",
    "migratedb": "dotenv -- yarn prisma migrate save --experimental && yarn prisma migrate up --experimental && yarn prisma generate",
    "studio": "dotenv -- npx prisma studio",
    "eslint": "node ./node_modules/eslint/bin/eslint.js --ext .ts,.tsx ."
  },
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-rc.8",
    "@chakra-ui/theme": "^1.0.0-rc.8",
    "@prisma/client": "^2.10.2",
    "favicons": "^6.2.0",
    "framer-motion": "^2.9.4",
    "heroicons-react": "^1.3.0",
    "next": "^9.5.5",
    "react": "^16.14.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.10.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.10.2",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.53",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "license": "MIT"
}
